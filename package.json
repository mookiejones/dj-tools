{
  "name": "dj-tools",
  "version": "0.0.1",
  "description": "A minimal Electron application written with Typescript",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm-run-all build electron",
    "electron": "electron --scripts-prepend-node-path ./dist/main.js"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "electron": "^9.2.0",
    "electron-log": "^4.2.4",
    "electron-settings": "^4.0.2",
    "glob": "^7.1.6",
    "update-electron-app": "^1.5.0"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/electron-settings": "^3.1.1",
    "@types/glob": "^7.1.3",
    "@types/node": "12",
    "npm-run-all": "^4.1.5",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  }
}